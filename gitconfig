[core]
	quotepath = false
[alias]
	logp = log -p
	sync = !git fetch && git rebase
	bow = !br="$(git rev-parse --abbrev-ref HEAD)" && git checkout master && git branch -d "$br"
[branch]
	autosetuprebase = always
[color]
	ui
[log]
	messageWrap = true
[push]
	default = upstream
[pull]
	rebase = true
[rebase]
	autoStash = true
